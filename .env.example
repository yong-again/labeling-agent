# DINO-SAM Labeling Agent Configuration
# Copy this file to .env and modify as needed:
#   cp .env.example .env

# Model paths (optional - uses defaults if not set)
# DINO_CONFIG_PATH=/workspace/labeling-agent/agent/model_config/GroundingDINO_SwinT_OGC.py
# DINO_CHECKPOINT_PATH=/workspace/labeling-agent/weights/groundingdino_swint_ogc.pth
# SAM_CHECKPOINT_PATH=

# Device selection (optional - auto-detects if not set)
# DEVICE=cuda

# Labeling settings
CONFIDENCE_THRESHOLD=0.35
BATCH_SIZE=1

# Output settings
OUTPUT_FORMAT=coco
OUTPUT_DIR=./output

# HITL (Human-In-The-Loop) settings
HITL_ENABLED=true
FEEDBACK_DB_PATH=./feedback.db

# Web server settings
HOST=0.0.0.0
PORT=8000
UPLOAD_DIR=./uploads
